{
  "compilerOptions": {
    "declaration": true,
    "module": "commonjs",
    "strict": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "jsx": "react-native",
    "lib": ["dom", "es5", "es6", "scripthost"],
    "target": "es5",
    "outDir": "lib",
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "resolveJsonModule": true
  },
  "exclude": ["node_modules", "example"]
}
